<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daoshengwanwu.android.tourassistant">

    <!-- 用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <!-- 用于访问GPS定位 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!-- 用于获取运营商信息，用于支持提供运营商信息相关的接口 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 用于访问wifi网络信息，wifi信息会用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 用于获取wifi的获取权限，wifi信息会用来进行网络定位 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <!-- 用于访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 用于读取手机当前的状态 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 用于写入缓存数据到扩展存储卡 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 用于申请调用A-GPS模块 -->
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="2214f6ab633dcb98ae9dc3e06256b9b5" />

        <activity android:name=".baihaoran.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".caizihuan.MyTrackActivity" />
        <activity android:name=".caizihuan.SearchMemberActivity" />
        <activity android:name=".caizihuan.ChatActivity" />
        <activity android:name=".caizihuan.ChatTableActivity" />
        <activity android:name=".jiangshengda.SearchActivity" />
        <activity android:name=".shenyue.PictureActivity" />
        <activity android:name=".shenyue.ViewspotActivity" />
        <activity android:name=".shenyue.ScenicspotActivity" />
        <activity android:name="com.daoshengwanwu.android.tourassistant.zhu.PersonalDataActivity" />
        <activity android:name="com.daoshengwanwu.android.tourassistant.zhu.SetActivity" />

        <activity android:name=".zhouxiuya.WelcomeActivity" />
        <activity android:name=".zhouxiuya.AboutActivity" />
        <activity android:name=".zhouxiuya.CollectActivity" />
        <activity android:name=".leekuo.MyTeamActivity" />
        <activity android:name=".leekuo.TransferTeamActivity" />
        <activity android:name=".leekuo.TeamMemberActivity" />
        <activity android:name=".leekuo.TeamerMyTeamActivity" />
        <activity android:name=".wangxiao.RegisterActivity" />
        <activity android:name=".wangxiao.Register2Activity" />
        <activity android:name=".wangxiao.LoginActivity" />

        <service android:name="com.amap.api.location.APSService" />
        <service android:name=".baihaoran.SharingService" />
    </application>

</manifest>